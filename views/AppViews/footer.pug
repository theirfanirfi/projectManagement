#myModal.modal.fade(tabindex='-1' role='dialog' aria-labelledby='myModalLabel' aria-hidden='true')
  .modal-dialog
    .modal-content
      .modal-header
        button.close(type='button' data-dismiss='modal' aria-hidden='true') &times;
        h4#myModalLabel.modal-title Create Project
      .modal-body
        form(action='/users/createProject' method='post' id='formsub')
          input.form-control(type='text' name='project_title' placeholder='Project Title' style='margin:12px;')
          label(style='margin-left:12px;') Project Description           
          textarea.form-control(name='project_description' style='margin:12px;height:150px;')
      .modal-footer
        button.btn.btn-primary(type='submit' id='sub') Create
